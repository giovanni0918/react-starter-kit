{
  "name": "react-starter-kit",
  "version": "1.1.0",
  "description": "A React.js progressive-web-app starter-kit, built using Webpack module bundler and react-router.",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev && npm run serve",
    "build": "NODE_ENV=production webpack -p --config webpack.config.js --colors",
    "dev": "webpack --mode=development --config webpack.config.js --colors",
    "serve": "node ./server.js",
    "windows-build": "set \"NODE_ENV=production\" && webpack -p --config webpack.config.js --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giovanni0918/react-starter-kit.git"
  },
  "keywords": [
    "react",
    "progressive-web-app",
    "starter-kit",
    "webpack",
    "react-router",
    "giovanni0918",
    "giovanniorlando"
  ],
  "author": "Giovanni Orlando Rivera <gorlandor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/giovanni0918/react-starter-kit/issues"
  },
  "homepage": "https://github.com/giovanni0918/react-starter-kit#readme",
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/runtime": "^7.7.7",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^4.3.1",
    "workbox-sw": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-react-constant-elements": "^7.7.4",
    "@babel/plugin-transform-react-inline-elements": "^7.7.4",
    "@babel/plugin-transform-react-jsx": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.1",
    "file-loader": "^5.0.2",
    "style-loader": "^1.1.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
