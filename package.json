{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "A React.js progressive-web-app starter-kit, built using Webpack module bundler and react-router.",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev & npm run serve",
    "build": "NODE_ENV=production webpack -p --config webpack.config.js --colors",
    "dev": "webpack --config webpack.config.js --colors & npm run serve",
    "serve": "node ./server.js",
    "windows-build": "set \"NODE_ENV=production\" && webpack -p --config webpack.config.js --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giovanni0918/react-starter-kit.git"
  },
  "keywords": [
    "react",
    "progressive-web-app",
    "starter-kit",
    "webpack",
    "react-router",
    "giovanni0918",
    "giovanniorlando"
  ],
  "author": "Giovanni Orlando Rivera <gorlandor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/giovanni0918/react-starter-kit/issues"
  },
  "homepage": "https://github.com/giovanni0918/react-starter-kit#readme",
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.16.0",
    "compression": "^1.6.2",
    "css-loader": "^0.28.0",
    "express": "^4.14.0",
    "file-loader": "^0.11.1",
    "json-loader": "^0.5.4",
    "prop-types": "^15.5.8",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router-dom": "^4.1.1",
    "style-loader": "^0.16.1",
    "sw-precache": "^5.1.0",
    "sw-toolbox": "^3.4.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
